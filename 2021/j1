import java.util.Scanner;
public class Main {
		public static void main(String[] args) {
			Scanner input = new Scanner(System.in);
			int b = input.nextInt();
			input.close();
			
			int p = 5*b - 400;
			
			int sl;
			if (b == 100){
					sl = 0;
				}else if (b<100) {
					sl = 1;
				}else{
					sl = -1;
				}
			
			System.out.println(p);
			System.out.println(sl);
		}
}
